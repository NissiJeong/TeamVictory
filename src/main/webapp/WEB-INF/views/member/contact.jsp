      <div class="row">
        <div class="col-lg-8">
          <div class="contact-form-area">
            <form class="cmn-form contact-form">
              <div class="row">
                <div class="col-md-6">
                  <div class="frm-group">
                    <input type="text" name="c_fname" id="c_fname" placeholder="First Name*">
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="frm-group">
                    <input type="text" name="c_lname" id="c_lname" placeholder="Last Name*">
